{
  "name": "app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "postinstall": "npx cap sync",
    "dev": "vite",
    "dev:ios": "pnpm build && chmod +x shells/run-ios.sh && ./shells/run-ios.sh",
    "dev:android": "pnpm build && NODE_ENV=development npx cap run android",
    "dev:app": "pnpm dev:ios && pnpm dev:android",
    "build": "chmod +x shells/update-version-from-package.sh && ./shells/update-version-from-package.sh && tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@capacitor-community/safe-area": "6.0.0-alpha.8",
    "@capacitor/android": "^6.2.0",
    "@capacitor/core": "^6.2.0",
    "@capacitor/ios": "^6.2.0",
    "@rocket/ui": "workspace:*"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.0",
    "@rocket/eslint-config": "workspace:*",
    "@rocket/tailwind-config": "workspace:*"
  },
  "volta": {
    "node": "22.11.0"
  },
  "packageManager": "pnpm@9.14.2"
}
